  "license": "GPL-3.0",
  "scripts": {
    "dev": "gulp clean && NODE_ENV=development gulp build && (ELEVENTY_ENV=development eleventy --serve & NODE_ENV=development gulp watch)",
    "prod": "yarn updatedb && gulp clean && NODE_ENV=production gulp build && ELEVENTY_ENV=production eleventy",
    "build": "yarn updatedb && gulp clean && NODE_ENV=development gulp build && ELEVENTY_ENV=development eleventy",
    "clean": "gulp clean",
    "updatedb": "yarn browserslist --update-db"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewpucci/andrewpucci.com"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-img": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.15",
    "delete": "^1.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-security": "^1.7.1",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^9.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-terser": "^2.1.0",
