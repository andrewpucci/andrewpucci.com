{
  "name": "andrewpucci.com",
  "version": "6.1.0",
  "description": "A personal portfolio site for Andrew Pucci",
  "author": "Andrew Pucci",
  "license": "GPL-3.0",
  "scripts": {
    "build:updatedb": "npx update-browserslist-db@latest",
    "build:clean": "rimraf dist",
    "build:tokens": "npx style-dictionary build -c './styledictionary.config.js'",
    "build": "run-s build:**",
    "run:dev:tokens": "npx onchange 'src/site/_tokens/**/*.json' -- npm run build:tokens",
    "run:dev:server": "NODE_ENV=development eleventy --serve",
    "run:dev": "run-p run:dev:**",
    "run:prod": "NODE_ENV=production eleventy",
    "dev": "run-s build run:dev",
    "prod": "run-s build run:prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewpucci/andrewpucci.com"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^4.0.2",
    "autoprefixer": "^10.4.19",
    "cssnano": "^6.1.1",
    "dotenv": "^16.4.5",
    "eleventy-plugin-rev": "^2.0.0",
    "eleventy-sass": "^2.2.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-security": "^2.1.1",
    "html-minifier": "^4.0.0",
    "luxon": "^3.4.4",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "postcss": "^8.4.38",
    "rimraf": "^5.0.5",
    "sass": "^1.72.0",
    "style-dictionary": "^3.9.2",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.2.1",
    "terser": "^5.29.2"
  }
}
