---
layout: base.njk
---
<main>
  <div class="container py-5">
    <article>
      {{ content | safe }}
      <div class="row d-flex flex-wrap">
        {% for item in cards %}
        <div class="col-md-6 col-lg-4 d-flex mb-3">
          {% card item.title, item.content, item.url, item.imgurl, item.title, "card-img-top h-auto", "410", [410, 820, null] %}
        </div>
        {% endfor %}
      </div>
    </article>
  </div>
</main>
